#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/projects/ITRDesign/lemarc-itr/node_modules/.pnpm/group-css-media-queries@1.4.1/node_modules/group-css-media-queries/bin/node_modules:/mnt/d/projects/ITRDesign/lemarc-itr/node_modules/.pnpm/group-css-media-queries@1.4.1/node_modules/group-css-media-queries/node_modules:/mnt/d/projects/ITRDesign/lemarc-itr/node_modules/.pnpm/group-css-media-queries@1.4.1/node_modules:/mnt/d/projects/ITRDesign/lemarc-itr/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/projects/ITRDesign/lemarc-itr/node_modules/.pnpm/group-css-media-queries@1.4.1/node_modules/group-css-media-queries/bin/node_modules:/mnt/d/projects/ITRDesign/lemarc-itr/node_modules/.pnpm/group-css-media-queries@1.4.1/node_modules/group-css-media-queries/node_modules:/mnt/d/projects/ITRDesign/lemarc-itr/node_modules/.pnpm/group-css-media-queries@1.4.1/node_modules:/mnt/d/projects/ITRDesign/lemarc-itr/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../group-css-media-queries/bin/group-css-media-queries" "$@"
else
  exec node  "$basedir/../group-css-media-queries/bin/group-css-media-queries" "$@"
fi
